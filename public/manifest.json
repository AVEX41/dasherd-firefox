{
    "manifest_version": 2,
    "name": "Custom New Tab Dashboard",
    "version": "1.0",
    "description": "Replaces new tab with a Svelte-powered dashboard.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "48": "public/vite.svg"
    },
    "permissions": [
        "storage"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "dashboard@example.com",
            "strict_min_version": "78.0"
        }
    }
}